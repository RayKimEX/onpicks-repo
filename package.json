{
  "name": "onpicks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "startko": "ng serve --configuration=local-ko --public-host http://localhost:4200",
    "starten": "ng serve --configuration=local-en --public-host http://localhost:4300 --port=4300",
    "proxystarten": "ng serve --configuration=local-en --proxy-config proxy.conf.json --public-host http://localhost:4200",
    "proxystartko": "ng serve --configuration=local-ko --proxy-config proxy.conf.json --public-host http://localhost:4200",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod --configuration=production-ko && ng run onpicks:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
